Cell[CellGroupData[{
Cell["Compute half-mass", "Subsection",
 CellChangeTimes->{{3.741093524080818*^9, 3.741093554324974*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"mhalf", "=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"\[Tau]", ",", "X"}], "}"}], " ", "\[Element]", "Reals"}], 
      " ", "&&", " ", 
      RowBox[{"\[Tau]", " ", ">", "1"}], "&&", 
      RowBox[{"X", ">", "0"}]}], ",", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        FractionBox["x", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "+", "x"}], ")"}], "2"]], 
        RowBox[{"(", 
         FractionBox[
          SuperscriptBox["\[Tau]", "2"], 
          RowBox[{
           SuperscriptBox["\[Tau]", "2"], "+", 
           SuperscriptBox["x", "2"]}]], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", "X"}], "}"}]}], "]"}]}], "]"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.7410912005722733`*^9, 3.741091315477048*^9}, {
   3.741091513075446*^9, 3.741091514220525*^9}, 3.7410935116188717`*^9, {
   3.741093603223885*^9, 3.74109360441967*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", 
   RowBox[{"2", " ", 
    RowBox[{"(", 
     RowBox[{"1", "+", "X"}], ")"}], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       SuperscriptBox["\[Tau]", "2"]}], ")"}], "2"]}]], 
  RowBox[{
   SuperscriptBox["\[Tau]", "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], " ", "X", " ", 
      RowBox[{"(", 
       RowBox[{"1", "+", 
        SuperscriptBox["\[Tau]", "2"]}], ")"}]}], "+", 
     RowBox[{"4", " ", 
      RowBox[{"(", 
       RowBox[{"1", "+", "X"}], ")"}], " ", "\[Tau]", " ", 
      RowBox[{"ArcTan", "[", 
       FractionBox["X", "\[Tau]"], "]"}]}], "+", 
     RowBox[{"2", " ", 
      RowBox[{"(", 
       RowBox[{"1", "+", "X"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "1"}], "+", 
        SuperscriptBox["\[Tau]", "2"]}], ")"}], " ", 
      RowBox[{"Log", "[", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "+", "X"}], ")"}], " ", "\[Tau]"}], "]"}]}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", "X"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "1"}], "+", 
        SuperscriptBox["\[Tau]", "2"]}], ")"}], " ", 
      RowBox[{"Log", "[", 
       RowBox[{
        SuperscriptBox["X", "2"], "+", 
        SuperscriptBox["\[Tau]", "2"]}], "]"}]}]}], ")"}]}]}]], "Output",
 CellChangeTimes->{3.741091317962226*^9, 3.7410915153614483`*^9, 
  3.741093560859289*^9, 3.741093607086465*^9}]
}, Open  ]]
