Cell[CellGroupData[{

Cell["Ok they intersect, find solution numerically", "Subsection",
 CellChangeTimes->{{3.741093718614553*^9, 3.7410937322253027`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"tauSol", "[", "X_", "]"}], ":=", 
  RowBox[{"FindRoot", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", "X"}], ")"}], " ", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "+", 
            SuperscriptBox["\[Tau]", "2"]}], ")"}], "2"]}]], 
       SuperscriptBox["\[Tau]", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], " ", "X", " ", 
          RowBox[{"(", 
           RowBox[{"1", "+", 
            SuperscriptBox["\[Tau]", "2"]}], ")"}]}], "+", 
         RowBox[{"4", " ", 
          RowBox[{"(", 
           RowBox[{"1", "+", "X"}], ")"}], " ", "\[Tau]", " ", 
          RowBox[{"ArcTan", "[", 
           FractionBox["X", "\[Tau]"], "]"}]}], "+", 
         RowBox[{"2", " ", 
          RowBox[{"(", 
           RowBox[{"1", "+", "X"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", 
            SuperscriptBox["\[Tau]", "2"]}], ")"}], " ", 
          RowBox[{"Log", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "+", "X"}], ")"}], " ", "\[Tau]"}], "]"}]}], "-", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "+", "X"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", 
            SuperscriptBox["\[Tau]", "2"]}], ")"}], " ", 
          RowBox[{"Log", "[", 
           RowBox[{
            SuperscriptBox["X", "2"], "+", 
            SuperscriptBox["\[Tau]", "2"]}], "]"}]}]}], ")"}]}], ")"}], " ", "==", 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[Tau]", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Pi]", "-", "\[Tau]"}], ")"}], " ", "\[Tau]"}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", 
            SuperscriptBox["\[Tau]", "2"]}], ")"}], " ", 
          RowBox[{"Log", "[", "\[Tau]", "]"}]}]}], ")"}]}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SuperscriptBox["\[Tau]", "2"]}], ")"}], "2"]]}], ",", 
    RowBox[{"{", 
     RowBox[{"\[Tau]", ",", 
      SuperscriptBox["X", "1.7"]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.741092050590312*^9, 3.741092289346614*^9}, {
  3.74109232872937*^9, 3.741092417556664*^9}, {3.741092568243325*^9, 
  3.741092571681553*^9}}]
