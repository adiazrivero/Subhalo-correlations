Cell[CellGroupData[{

Cell["Compute total mass", "Subsection",
 CellChangeTimes->{{3.741093568655455*^9, 3.741093571272355*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"mtot", " ", "=", " ", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"\[Tau]", ",", "X"}], "}"}], " ", "\[Element]", "Reals"}], 
      " ", "&&", " ", 
      RowBox[{"\[Tau]", " ", ">", "1"}], "&&", 
      RowBox[{"X", ">", "0"}]}], ",", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        FractionBox["x", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "+", "x"}], ")"}], "2"]], 
        RowBox[{"(", 
         FractionBox[
          SuperscriptBox["\[Tau]", "2"], 
          RowBox[{
           SuperscriptBox["\[Tau]", "2"], "+", 
           SuperscriptBox["x", "2"]}]], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", "Infinity"}], "}"}]}], "]"}]}], "]"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.741093609675208*^9, 3.7410936199106207`*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[Tau]", "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "1"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Pi]", "-", "\[Tau]"}], ")"}], " ", "\[Tau]"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "1"}], "+", 
        SuperscriptBox["\[Tau]", "2"]}], ")"}], " ", 
      RowBox[{"Log", "[", "\[Tau]", "]"}]}]}], ")"}]}], 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{"1", "+", 
     SuperscriptBox["\[Tau]", "2"]}], ")"}], "2"]]], "Output",
 CellChangeTimes->{{3.741093615387738*^9, 3.741093621095234*^9}}]
}, Open  ]]
